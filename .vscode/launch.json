{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Update-ContainerAppCors.ps1",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}/deploy/scripts/Update-ContainerAppCors.ps1",
      "args": ["-ResourceGroupName adoptrix-demo-rg"],
      "cwd": "${cwd}"
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}
