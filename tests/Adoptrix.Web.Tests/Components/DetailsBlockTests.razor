@inherits TestContext

@code {

    [Fact]
    public void Render_WithPropertiesSet_ShouldHaveExpectedMarkup()
    {
        // arrange
        const string title = "Test";
        const int elevation = 3;
        var component = Render(@<DetailsBlock Title="@title" Elevation="elevation"/>);

        // assert
        component.Find("h5").FirstChild!.MarkupMatches(title);
    }

}
